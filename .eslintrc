{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "no-console": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
